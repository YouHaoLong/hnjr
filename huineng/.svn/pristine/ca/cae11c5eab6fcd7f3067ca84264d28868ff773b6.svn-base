	color: #fff;
	letter-spacing: 1px;
	float: left !important;
}

.navbar-nav>li.title span{
	margin-left: 1%;
	font-size: 18px;
}
.navbar-nav>li.renwu{
	
	padding: 35px 1%;
	font-size: 14px;
	color: #fff;
	letter-spacing: 1px;
}
.navbar-nav>li.renwu span{
	color: #ffe851;
}
.navbar-nav>li>a{
	padding-top: 0;
	color: #fff !important;
	padding-bottom: 0;
}

}
.main .page-header {
  margin-top: 0;
}


